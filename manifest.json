{
    "manifest_version": 2,
    "name": "Sinônimos",
    "version": "1.0.0",
    "description": "Ache sinônimos para as suas palavras.",
    "short_name": "Sinônimos",
    "permissions": [
        "<all_urls>",
        "contextMenus",
        "tabs"
